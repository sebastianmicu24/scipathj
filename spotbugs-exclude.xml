<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Exclude GUI-related false positives -->
    <Match>
        <Class name="~.*\.ui\..*"/>
        <Bug pattern="SE_BAD_FIELD"/>
    </Match>
    
    <!-- Exclude Swing serialization warnings -->
    <Match>
        <Class name="~.*\.ui\..*"/>
        <Bug pattern="SE_NO_SERIALVERSIONID"/>
    </Match>
    
    <!-- Exclude TensorFlow native library warnings -->
    <Match>
        <Class name="~.*\.engine\..*"/>
        <Bug pattern="DM_EXIT"/>
    </Match>
    
    <!-- Exclude ImageJ plugin warnings -->
    <Match>
        <Package name="~de\.csbdresden\..*"/>
        <Bug pattern="NM_SAME_SIMPLE_NAME_AS_SUPERCLASS"/>
    </Match>
    
    <!-- Exclude utility class constructor warnings for constants classes -->
    <Match>
        <Class name="~.*Constants"/>
        <Bug pattern="UWF_UNWRITTEN_FIELD"/>
    </Match>
</FindBugsFilter>